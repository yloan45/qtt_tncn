<html lang="en">

<head>
  <%- include('./partials/head') %>
</head>

<body>
  <div class="container">
    <div class="khungtong">
      <!--phan header-->
      <header>
        <%- include('./partials/header') -%>
      </header>
      <div class="container-fluid" style="width: 450px; margin-top: 1pc; margin-bottom: 1pc;">
        <h4 style="text-align: center; margin-bottom: 3pc;">Nhập thông tin tra cứu</h4>
        <form id="captchaForm" action="/tra-cuu-to-khai-qtt" method="post">
          <div class="row mt-3">
            <div class="col" style="margin-bottom: 5px;">
              <span style="margin-bottom: 5px;">Tờ khai<font style="color: red;">(*)</font></span><br>
              <select name="tokhai" id=""  class="form-control">
                <option value="Tờ khai quyết toán thuế mẫu 02/QTT-TNCN">Tờ khai quyết toán thuế mẫu 02/QTT-TNCN</option>
              </select>
            </div>
          </div>
          <div class="row mt-3">
            <div class="col" style="margin-bottom: 5px;">
              <span style="margin-bottom: 5px;">Trạng thái<font style="color: red;">(*)</font></span><br>
              <select name="trangthaixuly" id=""  class="form-control">
                <option value="">-- Lựa chọn --</option>
                <option value="đang chờ duyệt"> Đang chờ duyệt</option>
                <option value="đã duyệt"> Đã duyệt </option>
                <option value="không được duyệt">Không được duyệt</option>
              </select>
            </div>
          </div>


          <div class="row mt-3">
            <div class="col" style="margin-bottom: 5px;">
              <span style="margin-bottom: 5px;">Ngày gửi thông báo<font style="color: red;">(*)</font></span>
              <input type="date" name="startDate" class="form-control">
              <% if (typeof startError!== 'undefined' && startError) { %>
                <span style="color: red;"><%= startError %></span>
              <% } %>
            </div>
          </div>

          <div class="row mt-3">
            <div class="col" style="margin-bottom: 5px;">
              <span style="margin-bottom: 5px;">Đến ngày<font style="color: red;">(*)</font></span>
              <input type="date" name="endDate" class="form-control">
              <% if (typeof endError!== 'undefined' && endError) { %>
                <span style="color: red;"><%= endError %></span>
              <% } %>
            </div>
          </div>
          <div style="text-align: center;" class="mt-3">
            <button  class="btn btn-primary" style="width: 150px;">Tra cứu</button>
          </div>
        </form>
        <br><br><br>
        <br><br><br>
        <!--PHẦN NỘI DUNG-->
      </div>
    </div>
  </div>
  <footer>
    <%- include('../partials/footer') %>
  </footer>
</body>

</html>