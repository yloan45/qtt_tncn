<html lang="en">

<head>
  <%- include('../partials/head') %>
</head>

<body>
  <div class="container">
    <div class="khungtong">
      <header>
        <%- include('../partials/header') -%>
      </header>
      <div class="container-fluid" style=" width: 900px;margin-top: 1pc; margin-bottom: 1pc;">
        <div class="card">
          <h5 class="card-header" style="text-align: center;  background-color: #2886d3; color: white; font-size: 16px;">Đăng ký tài khoản cá nhân</h5>
          <div class="card-body">
            <% if (messages.error) { %>
            <span style=" color: red;" class="error-message"><%= messages.error[0] %></span>
            <% } %>
            <form action="/canhan/dang-ky/b2" method="post">

              <div class="row mt-3">
                <div class="col"></div>
                <div class="col-3" style="text-align: right;">
                  <label for="" style="font-size: 16px; text-align: right;">Mã số thuế <font style="color: red;">(*)
                    </font></label>
                </div>
                <div class="col-7">
                  <input type="text" name="masothue" class="form-control" value="<%= user.masothue %>" style="background-color: white;" readonly>
                </div>
                <div class="col"></div>
              </div>

              <div class="row mt-3">
                <div class="col"></div>
                <div class="col-3" style="text-align: right;">
                  <label for="" style="font-size: 16px;">Tên cá nhân: <font style="color: red;">(*)
                    </font></label>
                </div>
                <div class="col-7">
                  <input type="text" name="fullname" class="form-control" value="<%= user.fullname %>" style="background-color: white;" readonly >
                </div>
                <div class="col"></div>
              </div>

              <div class="row mt-3">
                <div class="col"></div>
                <div class="col-3" style="text-align: right;">
                  <label for="" style="font-size: 16px;">Tỉnh/TP cư trú: <font style="color: red;">(*)
                    </font></label>
                </div>
                <div class="col-7">
                  <input type="text" name="tinh_tp" class="form-control" style="background-color: white;" readonly value="<%= user.dia_chi.tinh_tp %>">
                </div>
                <div class="col"></div>
              </div>


              <div class="row mt-3">
                <div class="col"></div>
                <div class="col-3" style="text-align: right;">
                  <label for="" style="font-size: 16px;">Cơ quan quản lý: <font style="color: red;">(*)
                    </font></label>
                </div>
                <div class="col-7">
                  <input type="text" name="cqqtthue" class="form-control" style="background-color: white;" readonly  value="<%= user.cqqtthue %>">
                </div>
                <div class="col"></div>
              </div>

              <div class="row mt-3">
                <div class="col"></div>
                <div class="col-3" style="text-align: right;">
                  <label for="" style="font-size: 16px;">Căn cước công dân: <font style="color: red;">(*)</font>
                  </label>
                </div>
                <div class="col-7">
                  <input type="text" name="cccd" class="form-control" style="background-color: white;" readonly value="<%= user.cccd %>">
                </div>
                <div class="col"></div>
              </div>

              <div class="row mt-3">
                <div class="col"></div>
                <div class="col-3" style="text-align: right;">
                  <label for="" style="font-size: 16px;">Số điện thoại: <font style="color: red;">(*)
                    </font></label>
                </div>
                <div class="col-7">
                  <input type="text" name="phone" class="form-control" style="background-color: white;" readonly value="<%= user.phone %>">
                </div>
                <div class="col"></div>
              </div>

              <div class="row mt-3">
                <div class="col"></div>
                <div class="col-3" style="text-align: right;">
                  <label for="" style="font-size: 16px;">Email: <font style="color: red;">(*)</font>
                  </label>
                </div>
                <div class="col-7">
                  <input type="text" name="email" class="form-control" value="<%= user.email %>" required>
                </div>
                <div class="col"></div>
              </div>
          </div>


          <div class="container text-center mt-5">
            <a href="/otp" class="btn mx-auto" style="width: 150px; background-color: #2886d3; color: white;">Quay lại</a>
            <button class="btn mx-auto" style="width: 150px; background-color: #2886d3; color: white;">Tiếp tục</button>
          </div>
          <br><br><br>
          </form>
        </div>
      </div>
      <br><br><br>
    </div>
  </div>
  <footer>
    <%- include('../partials/footer') %>
  </footer>
  <script src="js/index.js"></script>

</body>

</html>